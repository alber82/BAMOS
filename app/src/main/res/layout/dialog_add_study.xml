                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Investigador1"
                android:id="@+id/tvsinvestigador1" />

            <Spinner
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.8"
                android:id="@+id/spsinvestigador1"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.2"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Plan W Supp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_gravity="center"/>

            <Switch
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/swspwl"/>
            <Switch
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/swspwr"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                />
        </TableRow>
        <TableRow
            android:layout_marginTop="15dip">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Investigador2"
                android:id="@+id/tvsinvestigador2"/>

            <Spinner
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.8"
                android:id="@+id/spsinvestigador2"
                 />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
