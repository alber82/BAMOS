                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/spssdl"
                android:entries="@array/stepd"
                />

            <Spinner
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/spssdr"
                android:entries="@array/stepd"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
                />
        </TableRow>
