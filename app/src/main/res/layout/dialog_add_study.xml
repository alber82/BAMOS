                android:layout_height="wrap_content"
                android:layout_weight="1.5"
                android:id="@+id/spstrunk"
                android:entries="@array/ti"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.5"
